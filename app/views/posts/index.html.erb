<h1>タスク一覧</h1>
<%= link_to '新規投稿', new_post_path, class:"btn btn-info" %>
<br>
<table border="0">
<thead>
<tr>
<th>タイトル</th>
<th>投稿日時</th>
</tr>
</thead>
<tbody>
<% @posts.each do |post| %>
<tr>
<td><%= link_to post.title, post_path(post) %></td>
<td><%= post.updated_at %></td>
<td><%= link_to '編集',edit_post_path(post) %></td>
<td><%= link_to '削除', post_path(post),
method: :delete, class:"destroy",data:{confirm: '？'} %></td>
</tr>
<% end %>
</tbody>
</table>
